{
    "port": "3010",
    "latency": 30,
    "contentType": "application/json",
    "services": {
        "ping": {
            "contentType": "application/json",
            "responses": {
                "get": {
                    "status": 200,
                    "body": "json"
                 }
            }
        },
        "api/:id/services": {
            "contentType": "application/json",
            "responses": {
                "get": {
                    "status": 200,
                    "body": "{\"id\": 1, \"name\": \"service\"}"
                 }
            }
        },
        "api/:id/files": {
            "contentType": "application/json",
            "responses": {
                "get": {
                    "status": 200,
                    "bodyFile": "templates/mock/files.json"
                 }
            }
        },
        "search": {
            "responses": {
                "get": {
                    "dynamic": {
                        "random": {
                            "status": [200],
                            "body": ["ok"]
                        }
                    }
                 }
            }
        }
    }
}
